{
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "coreJs": 3,
          "targets": "> 0.25%, not dead"
        }
      ],
      ["@babel/preset-react"]
    ],
    "plugins": [
      ["@babel/proposal-class-properties"],
      ["@babel/proposal-object-rest-spread"],
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
}