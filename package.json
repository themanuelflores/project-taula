{
  "name": "taula",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "bundle": "webpack --display-error-details",
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "rm -rf dist/ && prettier --write src/ && eslint src --ext .tsx,.ts,.jsx,.js && tsc && webpack --config webpack.config.js --mode production",
    "build:dev": "rm -rf dist/ && prettier --write src/ && eslint src --ext .tsx,.ts,.jsx,.js && tsc && webpack --config webpack.config.js --env envFile=development --mode production",
    "lint": "eslint src --ext .tsx,.ts,.jsx,.js",
    "clean": "rm -rf dist/",
    "check-types": "tsc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "core-js": "^3.20.3",
    "d3": "^7.3.0",
    "d3-org-chart": "^2.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.4",
    "react-modal-hook": "^3.0.0",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/d3": "^7.1.0",
    "@types/d3-org-chart": "^2.3.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-plugin-react": "^7.28.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  }
